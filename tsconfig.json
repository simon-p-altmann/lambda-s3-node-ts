{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "resolveJsonModule": true,
    },
    "include": ["src/**/*.ts","src/*/*ts"],
    "exclude": ["node_modules", "dist","**/*.test.ts"]
  }
  